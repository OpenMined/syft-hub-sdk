# Application configuration
PROJECT_NAME=syft-nsai-sdk
PROJECT_VERSION=0.1.0
PROJECT_DESCRIPTION="SYFT NSAI SDK"
JWT_SECRET=
LOG_LEVEL=INFO
DEBUG=false

# SDK configuration - Required for SyftBox
SYFTBOX_CONFIG_PATH="~/.syftbox/config.json"
SYFTBOX_CACHE_SERVER_URL="https://syftbox.net"
SYFTBOX_AUTO_HEALTH_CHECK_THRESHOLD="10"
SYFTBOX_DEFAULT_MAX_COST="1.0"

# Development settings
SYFTBOX_DEBUG="true"
SYFTBOX_LOG_LEVEL="DEBUG"

# Accounting configuration
SYFTBOX_ACCOUNTING_EMAIL="example@example.com"
SYFTBOX_ACCOUNTING_PASSWORD="example_password"
SYFTBOX_ACCOUNTING_URL="accounting.url"